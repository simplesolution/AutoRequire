{
    "name": "simplesolution/auto-require",
    "description": "A Plugin which makes it easy to load private packages",
    "type": "composer-plugin",
    "keywords": [
        "composer",
        "composer-plugin",
        "private repositories",
        "auto require"
    ],
    "require": {
        "composer-plugin-api": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Franek",
            "email": "tobias.franek@simplesolution.at"
        }
    ],
    "autoload": {
        "psr-4": {
          "SimpleSolution\\": "src"
        }
      },
    "extra": {
        "class": "SimpleSolution\\AutoRequire\\AutoRequirePlugin"
    }
}
